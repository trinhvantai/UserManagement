<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>UserManagement</display-name>
  <welcome-file-list>
    <welcome-file>signUp.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>signupController</servlet-name>
    <servlet-class>controller.SignupController</servlet-class>
  </servlet>
   <servlet-mapping>
    <servlet-name>signupController</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>loginController</servlet-name>
    <servlet-class>controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>editController</servlet-name>
    <servlet-class>controller.EditController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>editController</servlet-name>
    <url-pattern>/edit</url-pattern>
  </servlet-mapping>
</web-app>